<h1>Student #<%= @student.id %></h1>

<dl>
  <dt>First name</dt>
  <dd><%= @student.first_name %></dd>

  <dt>Last name</dt>
  <dd><%= @student.last_name %></dd>

  <dt>Address</dt>
  <dd><%= @student.address %></dd>

  <dt>Image</dt>
  <dd><%= @student.image %></dd>

  <dt>Nu email</dt>
  <dd><%= @student.nu_email %></dd>

  <dt>Cell phone</dt>
  <dd><%= @student.cell_phone %></dd>

  <dt>Student</dt>
  <dd><%= @student.student_id %></dd>

</dl>
